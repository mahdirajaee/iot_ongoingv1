# Service Configuration
SERVICE_NAME=RaspberryPiConnector
SERVICE_PORT=8081

# Catalog Configuration
CATALOG_URL=http://localhost:8080
CATALOG_REGISTRATION_ENDPOINT=/api/catalog/register
CATALOG_GET_CONFIG_ENDPOINT=/api/catalog/config

# MQTT Configuration
MQTT_BROKER_HOST=localhost
MQTT_BROKER_PORT=1883
MQTT_CLIENT_ID=raspberry_pi_connector
MQTT_TOPIC_TEMPERATURE=/sensor/temperature
MQTT_TOPIC_PRESSURE=/sensor/pressure
MQTT_TOPIC_VALVE=/actuator/valve

# Sensor Configuration
SENSOR_PUBLISH_INTERVAL=5
TEMPERATURE_MEAN=70
TEMPERATURE_STD=5
PRESSURE_MEAN=100
PRESSURE_STD=10

# Actuator Configuration
VALVE_DEFAULT_STATE=closed